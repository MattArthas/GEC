create table GEC_INDEX_RATE
(
  INDEX_CD        VARCHAR2(4) not null,
  INDEX_DATE      NUMBER(8) not null,
  INDEX_RATE      NUMBER(9,6) default 0,
  LAST_UPDATED_BY VARCHAR2(64),
  LAST_UPDATED_AT DATE
)TABLESPACE GEC
;
alter table GEC_INDEX_RATE
  add ( constraint GEC_INDEX_RATE_PK primary key (INDEX_CD, INDEX_DATE)  USING INDEX  TABLESPACE GEC);
alter table GEC_INDEX_RATE
  add constraint GEC_INDEX_RATE_FK1 foreign key (INDEX_CD)
  references GEC_INDEX (INDEX_CD);

comment on table GEC_INDEX_RATE
  is 'Index Rate table.';
comment on column GEC_INDEX_RATE.INDEX_CD
  is 'N/A,FEDO,DFLT.';
