CREATE TABLE GEC_BULK_G1_RETURN
(
BULK_G1_RETURN_ID	NUMBER(38),
ASSET_ID 	NUMBER(38),
ASSET_CODE	VARCHAR2(20),
ASSET_CODE_TYPE	VARCHAR2(3),
TRANSACTION_TYPE	VARCHAR2(1),
TRANSACTION_CD	VARCHAR2(10),
TRADE_DATE	NUMBER(8),
SETTLE_DATE	NUMBER(8),
COUNTERPARTY_CD	VARCHAR2(6),
QTY	NUMBER(38),
STATUS	VARCHAR2(1),
BARGAIN_REF	VARCHAR2(200),
CREATED_BY	VARCHAR2(64),
CREATED_AT	DATE,
UPDATED_AT	DATE,
UPDATED_BY	VARCHAR2(64),
BRANCH_CD	VARCHAR2(10),
G1_INSTANCE_CD	VARCHAR2(40),
DIVIDEND_TRADE_FLAG	VARCHAR2(1),
LAST_UPDATED_AT	DATE,
LAST_UPDATED_BY	VARCHAR2(64)
) TABLESPACE GEC;

  
COMMENT ON TABLE GEC_BULK_G1_RETURN
  IS 'This table is used store bulk G1 return information from UI.';
COMMENT ON COLUMN GEC_BULK_G1_RETURN.BARGAIN_REF
  IS 'Borrow bargain references which user want to apply the borrow return.';
COMMENT ON COLUMN GEC_BULK_G1_RETURN.COUNTERPARTY_CD
  IS 'Value gets all lenders from Manage Lenders page and Fund NSB Counterparty from Manage Funds page.';
COMMENT ON COLUMN GEC_BULK_G1_RETURN.STATUS
  IS 'Values: P-ending (initial status), B-ooked, X-Error, E-Expired';
COMMENT ON COLUMN GEC_BULK_G1_RETURN.TRANSACTION_TYPE
  IS 'Values: B-orrow, L-oan,D for SB Broker.';
COMMENT ON COLUMN GEC_BULK_G1_RETURN.TRANSACTION_CD
  IS 'Values:BR';
COMMENT ON COLUMN GEC_BULK_G1_RETURN.DIVIDEND_TRADE_FLAG
  IS 'Values: N, Y';
ALTER TABLE GEC_BULK_G1_RETURN
  ADD CONSTRAINT GEC_BULK_G1_RETURN_PK PRIMARY KEY (BULK_G1_RETURN_ID)  USING INDEX TABLESPACE  GEC;
  
ALTER TABLE GEC_BULK_G1_RETURN
  ADD CONSTRAINT GEC_BULK_G1_RETURN_FK1 FOREIGN KEY (ASSET_ID)
  REFERENCES GEC_ASSET (ASSET_ID);