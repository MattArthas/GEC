CREATE TABLE GEC_AVAIL_BROADCAST_JOB
(
	BRAODCAST_JOB_ID			VARCHAR2(38),	
	STRATEGY_ID 				NUMBER(38), 
	BROADCAST_DATE				VARCHAR2(8) NOT NULL,
	BROADCAST_TIME				VARCHAR2(8) NOT NULL,
	ACTUAL_BROADCAST_DATE		VARCHAR2(8),
	ACTUAL_BROADCAST_TIME		VARCHAR2(8),
	BROADCASTED_FLAG			VARCHAR2(1),
	ERROR_MESSAGE				VARCHAR2(256),	
	CREATED_AT             		DATE default sysdate,
	CREATED_BY             		VARCHAR2(64),
	UPDATED_AT             		DATE,
	UPDATED_BY             		VARCHAR2(64),
	LAST_UPDATED_AT        		DATE,
	LAST_UPDATED_BY        		VARCHAR2(64)
) TABLESPACE GEC
;

COMMENT ON COLUMN GEC_AVAIL_BROADCAST_JOB.BROADCAST_DATE
  IS 'BROADCAST DATE,YYYYMMDD NOT NULL.';
COMMENT ON COLUMN GEC_AVAIL_BROADCAST_JOB.BROADCAST_TIME
  IS 'BROADCAST TIME';
COMMENT ON COLUMN GEC_AVAIL_BROADCAST_JOB.ACTUAL_BROADCAST_DATE
  IS 'ACTUAL BROADCAST DATE,YYYYMMDD NOT NULL.';
COMMENT ON COLUMN GEC_AVAIL_BROADCAST_JOB.ACTUAL_BROADCAST_TIME
  IS 'ACTUAL BROADCAST TIME';
COMMENT ON COLUMN GEC_AVAIL_BROADCAST_JOB.BROADCASTED_FLAG
  IS 'IS THE AVAILABILITY BROADCASTED? Y(ES),N(O)';
COMMENT ON COLUMN GEC_AVAIL_BROADCAST_JOB.ERROR_MESSAGE
  IS 'ERROR MESSAGE.';
  
ALTER TABLE GEC_AVAIL_BROADCAST_JOB
  ADD ( CONSTRAINT GEC_AVAIL_BROADCAST_JOB_PK PRIMARY KEY (BRAODCAST_JOB_ID) USING INDEX TABLESPACE GEC ) ;

ALTER TABLE GEC_AVAIL_BROADCAST_JOB
  ADD CONSTRAINT GEC_AVAIL_BROADCAST_JOB_FK1 FOREIGN KEY (STRATEGY_ID)
  REFERENCES GEC_STRATEGY (STRATEGY_ID);  
