CREATE TABLE GEC_ASSET_RATE (
	ASSET_ID		      NUMBER(38)		NOT NULL,
	ASSET_CODE		      VARCHAR2(20),
	ASSET_CODE_TYPE	      VARCHAR2(3),
	INDICATIVE_RATE	      NUMBER(9,6),
	POSITION_FLAG	      VARCHAR2(2),
	RESTRICTION_CD	      VARCHAR2(1),
	TYPE			      VARCHAR2(1),
    INTERNAL_COMMENT_TXT  VARCHAR2(4000),
	STATUS			      VARCHAR2(1),
	UPDATED_BY		      VARCHAR2(64),
	UPDATED_AT		      DATE,
	LAST_UPDATED_BY	      VARCHAR2(64),
	LAST_UPDATED_AT	      DATE
	) TABLESPACE GEC;
alter table GEC_ASSET_RATE
  add ( constraint GEC_ASSET_RATE_PK primary key (ASSET_ID) USING INDEX TABLESPACE  GEC);
ALTER TABLE GEC_ASSET_RATE 
  ADD CONSTRAINT GEC_ASSET_RATE_FK1 foreign key (ASSET_ID) REFERENCES GEC_ASSET (ASSET_ID); 
ALTER TABLE GEC_ASSET_RATE
  ADD CONSTRAINT GEC_ASSET_RATE_FK2 foreign key (RESTRICTION_CD) REFERENCES GEC_RESTRICTION (RESTRICTION_CD); 
  
comment on table GEC_ASSET_RATE
  is 'ASSET attribute table';
comment on column GEC_ASSET_RATE.ASSET_CODE
  is 'user input asset code, eg cusip/isin/sedol, and so on.';
comment on column GEC_ASSET_RATE.TYPE
  is 'Value: T-emporary, P-ermanent.';
comment on column GEC_ASSET_RATE.STATUS
  is 'Value: A-ctive, I-nactive, D-eleted. PS: currently, there will be only Active status in all records, but it is recommended that system check this status (A) when in SQL.';
comment on column GEC_ASSET_RATE.UPDATED_BY
  is 'The user id who updated the records. For system updating, it is null, and will display System in UI. When creating, the value is equal to the user ID of creater.';
comment on column GEC_ASSET_RATE.POSITION_FLAG
  is 'Values: GC, SP';
comment on column GEC_ASSET_RATE.INTERNAL_COMMENT_TXT
  is 'comment for internal users';
	