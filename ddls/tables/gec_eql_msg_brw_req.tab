create table GEC_EQL_MSG_BRW_REQ
(
	EQUILEND_MESSAGE_ID	 NUMBER(38),
	BORROW_REQUEST_ID	 NUMBER(38),
	LAST_UPDATED_AT	     DATE,
	LAST_UPDATED_BY 	 VARCHAR2(64)
)tablespace GEC;

COMMENT ON TABLE GEC_EQL_MSG_BRW_REQ
  IS 'It store the relatiosn ship between GEC_EQUILEND_MESSAGE and GEC_BORROW_REQUEST table.';

ALTER TABLE GEC_EQL_MSG_BRW_REQ ADD CONSTRAINTS GEC_EQL_MSG_BRW_REQ_PK
  PRIMARY KEY(EQUILEND_MESSAGE_ID) USING INDEX TABLESPACE GEC;
ALTER TABLE GEC_EQL_MSG_BRW_REQ ADD CONSTRAINTS GEC_EQL_MSG_BRW_REQ_FK1
  FOREIGN KEY(EQUILEND_MESSAGE_ID) references GEC_EQUILEND_MESSAGE(EQUILEND_MESSAGE_ID);
ALTER TABLE GEC_EQL_MSG_BRW_REQ ADD CONSTRAINTS GEC_EQL_MSG_BRW_REQ_FK2
  FOREIGN KEY(BORROW_REQUEST_ID) references GEC_BORROW_REQUEST(BORROW_REQUEST_ID);
