create table GEC_TRANSACTION
(
  TRANSACTION_CD   VARCHAR2(20) not null,
  TRANSACTION_DESC VARCHAR2(50),
  COMMENT_TXT      VARCHAR2(4000),
  LAST_UPDATED_BY  VARCHAR2(64),
  LAST_UPDATED_AT  DATE
)TABLESPACE GEC
;
alter table GEC_TRANSACTION
  add ( constraint GEC_TRANSACTION_PK primary key (TRANSACTION_CD)  USING INDEX TABLESPACE  GEC);


comment on column GEC_TRANSACTION.TRANSACTION_CD is 'COVER,LOCATE,SHORT,PREBORROW';
