create table GEC_TICKET_SIZE_RANGE
(
  TICKET_SIZE_FROM	  NUMBER(38,2),
  TICKET_SIZE_TO	    NUMBER(38,2),
  TICKET_SIZE_LABEL	  VARCHAR2(50),
  LAST_UPDATED_BY     VARCHAR2(64),
  LAST_UPDATED_AT     DATE
) tablespace GEC;

comment on table GEC_TICKET_SIZE_RANGE
  IS 'This table is used store ticket size range items for oder page filter.';
comment on column GEC_TICKET_SIZE_RANGE.TICKET_SIZE_LABEL
  is 'It is used for filter dropdown item literal display.';
            
alter table GEC_TICKET_SIZE_RANGE
  ADD CONSTRAINT GEC_TICKET_SIZE_RANGE_UK1 UNIQUE (TICKET_SIZE_FROM,TICKET_SIZE_TO);