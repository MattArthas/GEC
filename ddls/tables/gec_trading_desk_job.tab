CREATE TABLE GEC_TRADING_DESK_JOB	(
	DESK_CD			VARCHAR2(10)	NOT NULL,
	JOB_NAME		VARCHAR2(30)	NOT NULL,
	JOB_TIME_STR	VARCHAR2(30)	NOT NULL,
	LOCALE			VARCHAR2(64),
	STATUS	        VARCHAR2(1),
	STATUS_MSG	    VARCHAR2(50),
	JOB_COMMENT     VARCHAR2(250),
	LAST_UPDATED_BY	VARCHAR2(64),	
	LAST_UPDATED_AT	DATE	
	) TABLESPACE GEC;
ALTER TABLE GEC_TRADING_DESK_JOB
  ADD( CONSTRAINT GEC_TRADING_DESK_JOB_PK PRIMARY KEY (DESK_CD,JOB_NAME)  USING INDEX TABLESPACE  GEC);
ALTER TABLE GEC_TRADING_DESK_JOB 
  ADD CONSTRAINT GEC_TRADING_DESK_JOB_FK1 foreign key (DESK_CD) REFERENCES GEC_TRADING_DESK (TRADING_DESK_CD);  

comment on table GEC_TRADING_DESK_JOB
  is 'Trading Desk ';
comment on column GEC_TRADING_DESK_JOB.JOB_NAME
  is 'LOAD_AVAILABILITY_JOB_TIME, LOAD_LDAP_JOB_TIME, LOGOFF_JOB_TIME, LOCATE_EOD_JOB_TIME, DUPLICATE_TICKERS_JOB_TIME, AUTOMATE_PGP_FILE_JOB_TIME, etc.';	
comment on column GEC_TRADING_DESK_JOB.JOB_TIME_STR
  is 'The Quartz trigger time string, eg: 0/20 59 23 * * ?.';	
comment on column GEC_TRADING_DESK_JOB.LOCALE
  is 'It is the timezone for job_time_str, eg: America/New_York';	
comment on column GEC_TRADING_DESK_JOB.STATUS
  is 'W-aiting, P-rocessing, S-uccess, F-ail.';
comment on column GEC_TRADING_DESK_JOB.JOB_COMMENT 
is 'It is a brief description of this job. Application should not use this field.';
  
  
   
  
