CREATE TABLE GEC_BULK_G1_TRADE
(
BULK_G1_TRADE_ID	NUMBER(38),
ASSET_ID	NUMBER(38),
SETTLE_DATE	NUMBER(8),
TRADE_DATE	NUMBER(8),
STATUS	VARCHAR2(1),
STATUS_MSG	VARCHAR2(64),
G1_STATUS	VARCHAR2(1),
G1_STATUS_MSG	VARCHAR2(64),
TRANSACTION_TYPE	VARCHAR2(1),
COUNTERPARTY_CD	VARCHAR2(6),
SHARE_QTY	NUMBER(38),
PRICE	NUMBER(28,6),
RATE	NUMBER(14,6),
RECLAIM_RATE	NUMBER(9,8),
G1_EXTRACTED_AT	DATE,
G1_EXTRACTED_BY	VARCHAR2(64),
ASSET_CODE	VARCHAR2(20),
ASSET_CODE_TYPE	VARCHAR2(3),
DOMESTIC_TAX_PERCENTAGE	NUMBER(9,8),
PREPAY_DATE	NUMBER(8),
PREPAY_RATE	NUMBER(14,6),
COLLATERAL_CURRENCY_CD	VARCHAR2(3),
COMMENT_TXT	VARCHAR2(4000),
TERM_DATE	NUMBER(8),
MINIMUM_FEE	NUMBER(14,2),
MINIMUM_FEE_CD	VARCHAR2(3),
OVERSEAS_TAX_PERCENTAGE	NUMBER(9,8),
COLLATERAL_TYPE	VARCHAR2(1),
EXPECTED_RETURN_DATE	NUMBER(8),
CREATED_AT	         DATE,
CREATED_BY	         VARCHAR2(64),
UPDATED_AT	         DATE,
UPDATED_BY	         VARCHAR2(64),
CROSS_TRADE_FLAG	VARCHAR2(1) default 'N',
INTER_COMPANY_FLAG	VARCHAR2(1) default 'N',
LINK_REFERENCE      VARCHAR2(8),
BOOK_G1_BORROW_FLAG	VARCHAR2(1) default 'Y' not null,
LAST_UPDATED_AT	     DATE,
LAST_UPDATED_BY	     VARCHAR2(64)
) TABLESPACE GEC
;

COMMENT ON TABLE GEC_BULK_G1_TRADE
  IS 'This table is used for store bulk trade information';
  
COMMENT ON COLUMN GEC_BULK_G1_TRADE.STATUS
  IS 'Values: P-ending, E-rror, B-ooked, e-X-pired.';
  
COMMENT ON COLUMN GEC_BULK_G1_TRADE.STATUS_MSG
  IS 'Values: Pending, Error, Booked, Expired.';
  
COMMENT ON COLUMN GEC_BULK_G1_TRADE.G1_STATUS
  IS 'Values: N-o, Y-es.';
  
COMMENT ON COLUMN GEC_BULK_G1_TRADE.G1_STATUS_MSG
  IS 'Values: No, Yes.';
  
COMMENT ON COLUMN GEC_BULK_G1_TRADE.TRANSACTION_TYPE
  IS 'Values:B-orrow, L-oan.';
  
COMMENT ON COLUMN GEC_BULK_G1_TRADE.RATE
  IS 'The same as Fee / Rebate.';
  
COMMENT ON COLUMN GEC_BULK_G1_TRADE.RECLAIM_RATE
  IS 'The same as dividend rate. Allow user to input 9(3)V9(6) from UI , and store the value divide 100 to DB.';
  
COMMENT ON COLUMN GEC_BULK_G1_TRADE.DOMESTIC_TAX_PERCENTAGE
  IS 'The same as Dom Div Rate 9(3)v9(6), and store the value divide 100 to DB.';
  
COMMENT ON COLUMN GEC_BULK_G1_TRADE.COLLATERAL_TYPE
  IS 'Values: C, N, P';
  
COMMENT ON COLUMN GEC_BULK_G1_TRADE.COUNTERPARTY_CD
  IS 'Dropdown-for B, value will be from manage Lenders (except for SB Broker), SB lender will get from Fund SB Borrow Cpty.Dropdown-For L, value will be from mange fund SB Loan and NSB L cpty .';

COMMENT ON COLUMN GEC_BULK_G1_TRADE.CROSS_TRADE_FLAG
  IS 'Values: Y, N. A flag to mark the borrow/loan is cross borrow/loan or not.';

COMMENT ON COLUMN GEC_BULK_G1_TRADE.INTER_COMPANY_FLAG
  IS 'Values: Y, N. A flag to mark the borrow need generate cross borrow/loan.';
  
ALTER TABLE GEC_BULK_G1_TRADE
  ADD CONSTRAINT GEC_BULK_G1_TRADE_PK PRIMARY KEY (BULK_G1_TRADE_ID)  USING INDEX TABLESPACE  GEC;

ALTER TABLE GEC_BULK_G1_TRADE
  ADD CONSTRAINT GEC_BULK_G1_TRADE_FK1 FOREIGN KEY (ASSET_ID)
  REFERENCES GEC_ASSET (ASSET_ID);