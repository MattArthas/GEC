create  table GEC_G1_DML_BROKER_MAP
(
  G1_DML_BROKER_MAP_ID NUMBER(10),
  G1_COUNTERPARTY_CD  VARCHAR2(6) not null,
  DML_BROKER_CD       VARCHAR2(6) not null,
  LAST_UPDATED_BY     VARCHAR2(64),
  LAST_UPDATED_AT     DATE
)TABLESPACE GEC
;

alter table GEC_G1_DML_BROKER_MAP
    add  constraint GEC_G1_DML_BROKER_MAP_PK primary key(G1_DML_BROKER_MAP_ID) USING INDEX TABLESPACE GEC ;
comment on table GEC_G1_DML_BROKER_MAP
    is 'This is a map ......' ;
comment on column GEC_G1_DML_BROKER_MAP.G1_COUNTERPARTY_CD
    is 'It is broker_cd in gec_broker table for NSB brokers, eg: 0997. Trader will input/display the value in Borrow tab. G1(SFP) will use this code for booking.';
comment on column GEC_G1_DML_BROKER_MAP.DML_BROKER_CD
    is 'It is broker_cd in gec_broker table for NSB brokers, eg: 0002625. DML and Equilend will use this code for borrowing shares.';
