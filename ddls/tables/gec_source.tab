create table GEC_SOURCE
(
  SOURCE_CD       VARCHAR2(64) not null,
  SOURCE_DESC     VARCHAR2(50),
  COMMENT_TXT     VARCHAR2(4000),
  LAST_UPDATED_BY VARCHAR2(64),
  LAST_UPDATED_AT DATE
)TABLESPACE GEC
;
comment on table GEC_SOURCE
  is 'source code reference';
comment on column GEC_SOURCE.SOURCE_CD
  is 'Source code';
comment on column GEC_SOURCE.SOURCE_DESC
  is 'Source code description';

