CREATE TABLE GEC_MESSAGE_AUD
(
   MESSAGE_ID      NUMBER (38),
   REQUEST_ID      NUMBER (38),
   MESSAGE_TYPE    VARCHAR2(64),
   CREATED_AT      DATE,
   MESSAGE_CONTENT BLOB,
   SOURCE_CD       VARCHAR2(64),
   STATUS          VARCHAR2(1),
   LAST_UPDATED_BY VARCHAR2(64),
   LAST_UPDATED_AT DATE,
   OP_CODE CHAR(1)
)TABLESPACE GEC
LOB(MESSAGE_CONTENT) STORE AS GEC_MESSAGE_ALS(TABLESPACE GEC)
;
