create table GEC_ASSET_TYPE
(
	ASSET_TYPE_ID		VARCHAR2(1),
	ASSET_TYPE_DESC		VARCHAR2(256),
	UPDATED_BY			VARCHAR2(64),
	UPDATED_AT			DATE,
	LAST_UPDATED_BY		VARCHAR2(64),
	LAST_UPDATED_AT		DATE
) tablespace GEC
;

comment on column GEC_ASSET_TYPE.ASSET_TYPE_ID
  is 'PK.Securities type. values:4 -(Common Stock)';
comment on column GEC_ASSET_TYPE.ASSET_TYPE_DESC 
is 'Description for security type. Values:Common Stock'; 

alter table GEC_ASSET_TYPE
  add ( constraint GEC_ASSET_TYPE_PK primary key (ASSET_TYPE_ID) USING INDEX TABLESPACE GEC ) ;




