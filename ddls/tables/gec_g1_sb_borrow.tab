create table GEC_G1_SB_BORROW
(
  BORROW_ID       NUMBER(38) not null,
  COUNTERPARTY_CD VARCHAR2(6) not null,
  BORROW_QTY      NUMBER(38),
  LAST_UPDATED_BY VARCHAR2(64),
  LAST_UPDATED_AT DATE
)TABLESPACE GEC
;
alter table GEC_G1_SB_BORROW
  add ( constraint GEC_G1_SB_BORROW_PK primary key (BORROW_ID,COUNTERPARTY_CD) USING INDEX  TABLESPACE GEC);
alter table GEC_G1_SB_BORROW
  add constraint GEC_G1_SB_BORROW_FK1 foreign key (BORROW_ID)
  references GEC_BORROW (BORROW_ID);    

comment on table GEC_G1_SB_BORROW
  is 'This table is used to save G1 booked SB borrows.';
comment on column GEC_G1_SB_BORROW.COUNTERPARTY_CD
  is 'The value is from GEC_G1_BOOKING.COUNTERPARTY_CD';
  
