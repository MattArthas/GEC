create table GEC_JOB_LOG
(
  JOB_NAME   VARCHAR2(70) not null,
  SESSION_ID NUMBER(38) not null,
  START_TIME TIMESTAMP(3) not null,
  END_TIME   TIMESTAMP(3),
  STATUS     VARCHAR2(1),
  STATUS_MSG VARCHAR2(100)
)TABLESPACE GEC
;

comment on table GEC_JOB_LOG
  is 'Log table that records main activities of SP.';
  
create index GEC_JOB_LOG_IDX1 on GEC_JOB_LOG (JOB_NAME, SESSION_ID, START_TIME) TABLESPACE GEC;