create table GEC_SCHEDULE
(
	SCHEDULE_ID	            NUMBER(10),
	EQUILEND_SCHEDULE_ID	NUMBER(10) not null,
	SCHEDULE_DESC	        VARCHAR2(50),
	BROKER_CD	            VARCHAR2(6),
	SFP_CORP_ID             NUMBER(10),
	SFP_LEGAL_ENTITY_ID	    NUMBER(10),
	SFP_LEGAL_ENTITY_NAME	VARCHAR2(40),
	LENDER_CORP_ID	        NUMBER(10),
	LENDER_LEGAL_ENTITY_ID	NUMBER(10),
	LENDER_LEGAL_ENTITY_NAME	VARCHAR2(40),
	TYPE	                VARCHAR2(2),
	STATUS	                VARCHAR2(1),
	RATE_TYPE	            VARCHAR2(2),
	RECLAIM_RATE	        NUMBER(9,8),
	EQL_COLLATERAL_TYPE	    VARCHAR2(2),
	COLLATERAL_CURRENCY_CD	VARCHAR2(3),
	EQL_COLLATERAL_TYPE_DESC    VARCHAR2(3),
	BILLING_CURRENCY_CD	    VARCHAR2(3),
	SETTLE_TYPE	            VARCHAR2(3),
	GEC_STATUS              VARCHAR2(1),
	SUBACCOUNT_ID           VARCHAR2(10),
	UPDATED_AT	            DATE,
	UPDATED_BY	            VARCHAR2(64),
	LAST_UPDATED_AT	        DATE,
	LAST_UPDATED_BY	        VARCHAR2(64)
)tablespace GEC;

COMMENT ON TABLE GEC_SCHEDULE 
  IS 'It store the Equilend schedule information.';
COMMENT ON COLUMN GEC_SCHEDULE.GEC_STATUS
  IS 'Value is I-active/A-ctive';
COMMENT ON COLUMN GEC_SCHEDULE.EQUILEND_SCHEDULE_ID
  IS 'Equilend schedule id.';
COMMENT ON COLUMN GEC_SCHEDULE.TYPE  
  IS 'Value is AB/AL.';
COMMENT ON COLUMN GEC_SCHEDULE.STATUS
  IS 'Value is A - Active,I - Inactive,D - Declined,P - Pending to make Active,X - Pending to make Inactive.'; 
COMMENT ON COLUMN GEC_SCHEDULE.RATE_TYPE
  IS 'Value is BO, FI, BE'; 
COMMENT ON COLUMN GEC_SCHEDULE.RECLAIM_RATE
  IS 'Value should be divided 100, then store in DB, when using this value, select this value then multiply by 100.';  
COMMENT ON COLUMN GEC_SCHEDULE.SETTLE_TYPE
  IS 'Represents the settlement type to be used between the initiator and the counterparty.';
COMMENT ON COLUMN GEC_SCHEDULE.UPDATED_AT
  IS 'when inserting data set value upated_at = created_at, set updated_by = created_by.';      
COMMENT ON COLUMN GEC_SCHEDULE.SFP_LEGAL_ENTITY_ID
  IS 'Usually the legal_entity_name is determined by legal_entity_id, but equliend don"t guarantee same legal entity name for same legal entity id. So  it seem observe 3  normal form.';

ALTER TABLE   GEC_SCHEDULE ADD CONSTRAINTS GEC_SCHEDULE_PK
  PRIMARY KEY(SCHEDULE_ID) USING INDEX TABLESPACE GEC;
ALTER TABLE GEC_SCHEDULE ADD CONSTRAINTS GEC_SCHEDULE_UK
  UNIQUE(EQUILEND_SCHEDULE_ID)USING INDEX TABLESPACE GEC;
  