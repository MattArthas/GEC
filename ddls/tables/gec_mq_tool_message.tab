
create table GEC_MQ_TOOL_MESSAGE
(
  GEC_MQ_TOOL_MESSAGE_ID    NUMBER(10),
  QUEUE_NAME                VARCHAR2(100),
  MESSAGE_CONTENT           BLOB,
  CREATED_AT                DATE,
  CREATED_BY                VARCHAR2(64), 
  LAST_UPDATED_BY	          VARCHAR2(64),	
  LAST_UPDATED_AT	          DATE	
)TABLESPACE GEC
 LOB(MESSAGE_CONTENT) STORE AS GEC_MQ_TOOL_MESSAGE_LS(TABLESPACE GEC); 
 
 ALTER TABLE GEC_MQ_TOOL_MESSAGE ADD CONSTRAINTS 
  GEC_MQ_TOOL_MESSAGE_PK PRIMARY KEY(GEC_MQ_TOOL_MESSAGE_ID) USING INDEX TABLESPACE GEC;
  
 CREATE INDEX GEC_MQ_TOOL_MESSAGE_IDX1 ON GEC_MQ_TOOL_MESSAGE(QUEUE_NAME) TABLESPACE GEC ;
 COMMENT  ON COLUMN  GEC_MQ_TOOL_MESSAGE.QUEUE_NAME 
  IS 'THE QUENE NAME';
