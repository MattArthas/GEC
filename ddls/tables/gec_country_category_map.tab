CREATE TABLE GEC_COUNTRY_CATEGORY_MAP
(
  COUNTRY_CATEGORY_CD	VARCHAR2(10),
  COUNTRY_CD	VARCHAR2(3),
  LAST_UPDATED_AT	DATE,
  LAST_UPDATED_BY	VARCHAR2(64)
) TABLESPACE GEC
;
  
COMMENT ON TABLE GEC_COUNTRY_CATEGORY_MAP
  IS 'This table is used store mapping between country and country category.';
  
ALTER TABLE GEC_COUNTRY_CATEGORY_MAP
  ADD CONSTRAINT GEC_COUNTRY_CATEGORY_MAP_PK PRIMARY KEY (COUNTRY_CATEGORY_CD, COUNTRY_CD)  USING INDEX TABLESPACE  GEC;

ALTER TABLE GEC_COUNTRY_CATEGORY_MAP
  ADD CONSTRAINT GEC_COUNTRY_CATEGORY_MAP_FK1 FOREIGN KEY (COUNTRY_CATEGORY_CD) references GEC_COUNTRY_CATEGORY (COUNTRY_CATEGORY_CD); 

ALTER TABLE GEC_COUNTRY_CATEGORY_MAP
  ADD CONSTRAINT GEC_COUNTRY_CATEGORY_MAP_FK2 FOREIGN KEY (COUNTRY_CD) references GEC_TRADE_COUNTRY (TRADE_COUNTRY_CD); 