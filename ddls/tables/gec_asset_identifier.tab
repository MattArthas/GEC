create table GEC_ASSET_IDENTIFIER
(
  ASSET_CODE       VARCHAR2(20) not null,
  ASSET_CODE_TYPE  VARCHAR2(3) not null,
  ASSET_ID         NUMBER(38) not null,
  LAST_UPDATED_BY  VARCHAR2(64),
  LAST_UPDATED_AT  DATE
)tablespace GEC
;
alter table GEC_ASSET_IDENTIFIER
  add  constraint GEC_ASSET_IDENTIFIER_FK foreign key (ASSET_ID)
  references GEC_ASSET (ASSET_ID)  ;
create index GEC_ASSET_IDENTIFIER_IDX1 on GEC_ASSET_IDENTIFIER (ASSET_CODE, ASSET_CODE_TYPE) TABLESPACE GEC;

comment on column GEC_ASSET_IDENTIFIER.ASSET_CODE
  is 'Cusip or ISIN or SEDOL or QUICK.';
comment on column GEC_ASSET_IDENTIFIER.ASSET_CODE_TYPE
  is 'CSP:Cusip,ISN:ISIN,SED:SEDOL,QUK:QUICK,TIK: TICKER';
