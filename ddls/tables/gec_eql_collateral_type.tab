CREATE TABLE GEC_EQL_COLLATERAL_TYPE
(
	EQL_COLLATERAL_TYPE_ID       NUMBER(10),
	EQL_COLLATERAL_TYPE	    VARCHAR2(2) NOT NULL,
	SUBACCOUNT_ID                      VARCHAR2(10),
	EQL_COLLATERAL_TYPE_DESC	        VARCHAR2(3),
	GEC_COLLATERAL_TYPE	            VARCHAR2(4) NOT NULL,
	LAST_UPDATED_AT                 DATE,
	LAST_UPDATED_BY                 VARCHAR2(64)
)TABLESPACE GEC
;
ALTER TABLE GEC_EQL_COLLATERAL_TYPE 
ADD CONSTRAINT GEC_EQL_COLLATERAL_TYPE_PK PRIMARY KEY (EQL_COLLATERAL_TYPE_ID) USING INDEX TABLESPACE GEC;

ALTER TABLE GEC_EQL_COLLATERAL_TYPE
ADD CONSTRAINTS GEC_EQL_COLLATERAL_TYPE_FK FOREIGN KEY (GEC_COLLATERAL_TYPE) REFERENCES GEC_COLLATERAL_TYPE(COLLATERAL_TYPE);

CREATE UNIQUE INDEX GEC_EQL_COLLATERAL_TYPE_IDX1 ON GEC_EQL_COLLATERAL_TYPE(EQL_COLLATERAL_TYPE,SUBACCOUNT_ID,EQL_COLLATERAL_TYPE_DESC) TABLESPACE GEC;

COMMENT ON TABLE GEC_EQL_COLLATERAL_TYPE
  IS 'This table is used to conver external collateral type to GEC collateral type, one exteranl collateral type and one exteranl subtype can determine one GEC collateral type';
COMMENT ON COLUMN GEC_EQL_COLLATERAL_TYPE.GEC_COLLATERAL_TYPE
  IS 'Value is GEC collateral type: CASH and USTA';
COMMENT ON COLUMN GEC_EQL_COLLATERAL_TYPE.EQL_COLLATERAL_TYPE
  IS 'Value is external collateral type:CA,NC,CP and MX';
COMMENT ON COLUMN GEC_EQL_COLLATERAL_TYPE.SUBACCOUNT_ID
  IS 'EQUILEND COLL TYPE SUBACCOUNT_ID';  
COMMENT ON COLUMN GEC_EQL_COLLATERAL_TYPE.EQL_COLLATERAL_TYPE_DESC
  IS 'EQUILEND COLL TYPE Description';
