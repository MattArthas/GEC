create table GEC_EQL_MSG_FILE
(
 	EQUILEND_MESSAGE_ID	NUMBER(38),
	FILE_ID	NUMBER(38),
  LAST_UPDATED_AT	DATE,
  LAST_UPDATED_BY	VARCHAR2(64)

) tablespace GEC;

COMMENT ON TABLE GEC_EQL_MSG_FILE
  IS 'It store the realationship between GEC_FILE and GEC_EQUILEND_MESSAGE.';
comment on column GEC_EQL_MSG_FILE.FILE_ID
  is 'GEC_FILE"s FILE_ID.';
comment on column GEC_EQL_MSG_FILE.EQUILEND_MESSAGE_ID
  is 'GEC_EQUILEND_MESSAGE"s EQUILEND_MESSAGE_ID.';
  
ALTER TABLE GEC_EQL_MSG_FILE
  ADD CONSTRAINT GEC_EQL_MSG_FILE_PK PRIMARY KEY (EQUILEND_MESSAGE_ID)  USING INDEX TABLESPACE  GEC;

ALTER TABLE GEC_EQL_MSG_FILE 
  ADD CONSTRAINT GEC_EQL_MSG_FILED_FK FOREIGN KEY(EQUILEND_MESSAGE_ID) REFERENCES  GEC_EQUILEND_MESSAGE(EQUILEND_MESSAGE_ID);

ALTER TABLE GEC_EQL_MSG_FILE 
  ADD CONSTRAINT GEC_EQL_MSG_FILE_FK2 FOREIGN KEY(FILE_ID) REFERENCES  GEC_FILE(FILE_ID);
