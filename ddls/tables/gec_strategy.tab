create table GEC_STRATEGY
(
  STRATEGY_ID     			NUMBER(38) not null,
  CLIENT_ID       			NUMBER(38) not null,
  STRATEGY_NAME   			VARCHAR2(50) not null,
  STATUS          			VARCHAR2(1),
  STATUS_MSG				VARCHAR2(50),
  COMMENT_TXT     			VARCHAR2(4000),
  ST_STATUS					VARCHAR2(1),
  ST_STATUS_MSG				VARCHAR2(50),
  API_STATUS				VARCHAR2(1),
  API_STATUS_MSG			VARCHAR2(50),
  FILE_PATH					VARCHAR2(255),
  IM_DEFAULT_FUND_CD		VARCHAR2(4),
  TEMP_IM_DEFAULT_FUND_CD	VARCHAR2(4),
  RESPONSE_FILENAME_PREFIX	VARCHAR2(100),
  FILENAME_POSTFIX_PATTERN	VARCHAR2(100),
  GC_RATE_TYPE              VARCHAR2(1) default 'T' NOT NULL ,
  ST_GC_RATE_TYPE           VARCHAR2(1) default 'T' NOT NULL ,
  AVAILABILITY_TYPE 				VARCHAR2(16),
  AVAILABILITY_SB_RATE 				NUMBER(36,6),
  AVAILABILITY_INCLUDE_SP_FLAG 		VARCHAR2(1),
  AVAILABILITY_FILE_TYPE  			VARCHAR2(24),
  AVAILABILITY_RATE_TYPE 			VARCHAR2(1) default 'T' NOT NULL ,
  AVAILABILITY_BROADCAST_TIME 		VARCHAR2(8),
  AVAILABILITY_RATE_FACTOR 			NUMBER(28,2),
  AVAILABILITY_FILE_NAME_FORMAT  	VARCHAR2(128),
  SITE_ID                           VARCHAR2(64),
  LAST_UPDATED_BY 					VARCHAR2(64),
  LAST_UPDATED_AT 			DATE
)TABLESPACE GEC
;
 
alter table GEC_STRATEGY
  add ( constraint GEC_STRATEGY_PK primary key (STRATEGY_ID)  USING INDEX TABLESPACE  GEC);
alter table GEC_STRATEGY
  add constraint GEC_STRATEGY_FK1 foreign key (CLIENT_ID)
  references GEC_CLIENT (CLIENT_ID);

  
comment on column GEC_STRATEGY.STATUS
  is 'A-ctive, I-nactive, D-eleted.';
comment on column GEC_STRATEGY.ST_STATUS
  is 'This field is for File Automation. Values: NULL: initial value; O: automation; I: inactive; M: manually.';
comment on column GEC_STRATEGY.API_STATUS
  is 'A-ctive, I-nactive.For API.';
comment on column GEC_STRATEGY.GC_RATE_TYPE
  is 'Values: N-umber, T-ext.';
comment on column GEC_STRATEGY.ST_GC_RATE_TYPE
  is 'Values: N-umber, T-ext.';
comment on column GEC_STRATEGY.FILE_PATH 
is 'The parth/location in ST where gec locate response file should be put in.';
comment on column GEC_STRATEGY.IM_DEFAULT_FUND_CD 
is 'The default fund of the strategy. Default fund will be used in loading availabilities.';
comment on column GEC_STRATEGY.TEMP_IM_DEFAULT_FUND_CD 
is 'User-input default fund will take effect at next loading availability. This field is used to save the temp value of user-input. System will copy this value to  IM_DEFAULT_FUND_CD at next loading.';
comment on column GEC_STRATEGY.RESPONSE_FILENAME_PREFIX 
is 'This file name prefix will be used for ST/PGP LOCATE response file. Preborrow do not use this file prefix.';
comment on column GEC_STRATEGY.RESPONSE_FILENAME_PREFIX 
is 'System use the pattern to find the postfix of locate response file name. The symbol * will be used as postfix. Value sample: aqr_locate_request_*.csv';
comment on column GEC_STRATEGY.SITE_ID
  is 'An id provided by EC(Eze Castle) to tell which strategy/client the EC system want to locate for';

 
