create table GEC_EMPLOYEE_TYPE
(
  EMPLOYEE_ID_STARTS_WITH VARCHAR(10) not null,
  EMPLOYEE_TYPE VARCHAR(10) not null,
  LAST_UPDATED_BY VARCHAR2(64),
  LAST_UPDATED_AT DATE
)TABLESPACE GEC
;

alter table GEC_EMPLOYEE_TYPE
  add ( constraint GEC_EMPLOYEE_TYPE_PK primary key (EMPLOYEE_ID_STARTS_WITH)  USING INDEX  TABLESPACE GEC);
  
  comment on table GEC_EMPLOYEE_TYPE
  is 'Its Table for mapping the employee with its corresponding type,for example if the employee is contractor its corresponing type is CM';
comment on column GEC_EMPLOYEE_TYPE.EMPLOYEE_ID_STARTS_WITH is 'the first alphabet/degit of the employee Id column';