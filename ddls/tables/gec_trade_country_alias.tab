create table GEC_TRADE_COUNTRY_ALIAS
(
 	TRADE_COUNTRY_ALIAS_CD   	VARCHAR2(3),
 	TRADE_COUNTRY_CD   			VARCHAR2(3), 
 	LAST_UPDATED_BY			    VARCHAR2(64),	
	LAST_UPDATED_AT			    DATE
)TABLESPACE GEC
;
ALTER TABLE GEC_TRADE_COUNTRY_ALIAS
  ADD CONSTRAINTS GEC_TRADE_COUNTRY_ALIAS_PK PRIMARY KEY(TRADE_COUNTRY_ALIAS_CD) USING INDEX TABLESPACE GEC;
comment  on table GEC_TRADE_COUNTRY_ALIAS
is 'It is used to change trade country value from UK or GB.';
comment on column GEC_TRADE_COUNTRY_ALIAS.TRADE_COUNTRY_CD
is 'Value is "GB"';
comment on column GEC_TRADE_COUNTRY_ALIAS.TRADE_COUNTRY_ALIAS_CD
is 'Value is "UK"';
