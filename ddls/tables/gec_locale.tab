CREATE TABLE GEC_LOCALE(	
	LOCALE_ID           NUMBER(10)		NOT NULL,
	LOCALE   			VARCHAR2(50)	NOT NULL,
	LAST_UPDATED_BY		VARCHAR2(64),
	LAST_UPDATED_AT		DATE	
	) TABLESPACE GEC;
alter table GEC_LOCALE
  add ( constraint GEC_LOCALE_PK primary key (LOCALE_ID) USING INDEX TABLESPACE  GEC);
ALTER TABLE GEC_LOCALE
  add ( constraint GEC_LOCALE_UK unique (LOCALE)  USING INDEX TABLESPACE  GEC);
