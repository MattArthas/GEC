CREATE TABLE GEC_COLLATERAL_TYPE(	
	DISPLAY_ORDER   NUMBER(3) NOT NULL,
	COLLATERAL_TYPE_ID	NUMBER(10)      NOT NULL,
	COLLATERAL_TYPE		VARCHAR2(4)     NOT NULL,
	DESCRIPTION			VARCHAR2(50),
	LAST_UPDATED_BY		VARCHAR2(64),	
	LAST_UPDATED_AT		DATE	
	) TABLESPACE GEC;
alter table GEC_COLLATERAL_TYPE
  add ( constraint GEC_COLLATERAL_TYPE_PK primary key (COLLATERAL_TYPE_ID) USING INDEX TABLESPACE  GEC);
ALTER TABLE GEC_COLLATERAL_TYPE
  add ( constraint GEC_COLLATERAL_TYPE_UK unique (COLLATERAL_TYPE)  USING INDEX TABLESPACE  GEC);
	
comment on table GEC_COLLATERAL_TYPE
  is 'Collateral Type. ';
comment on column GEC_COLLATERAL_TYPE.COLLATERAL_TYPE_ID
  is 'sequence id. And do not use this as foreign key in GEC.';	
comment on column GEC_COLLATERAL_TYPE.COLLATERAL_TYPE
  is 'Should use this column as foreign key in other tables.';  
