CREATE TABLE GEC_TRADE_COUNTRY
(
	TRADE_COUNTRY_ID		NUMBER(10)		NOT NULL,
	TRADE_COUNTRY_CD		VARCHAR2(3)		NOT NULL,
	TRADE_COUNTRY_NAME		VARCHAR2(50),	
	CURRENCY_CD				VARCHAR2(3),
	PREPAY_DATE_VALUE		NUMBER(3),	
	CUTOFF_TIME				VARCHAR2(8),	
	TRADING_DESK_CD			VARCHAR2(10),	
	LOCALE					VARCHAR2(64),
	PREBORROW_ELIGIBLE_FLAG	VARCHAR2(1)		DEFAULT 'N',		
  	STATUS          		VARCHAR2(1),
  	G1_AUTO_BOOKING	        VARCHAR2(1)	NOT NULL,
  	GROUP_ALL_LOAN	        VARCHAR2(1)	NOT NULL,
  	VISIBLE_FLAG			VARCHAR2(1) DEFAULT 'N',
  	UPDATED_AT				DATE 		DEFAULT SYSDATE,
	UPDATED_BY				VARCHAR2(64),	
	LAST_UPDATED_BY			VARCHAR2(64),	
	LAST_UPDATED_AT			DATE
)  TABLESPACE GEC
;
ALTER TABLE GEC_TRADE_COUNTRY
  ADD( CONSTRAINT GEC_TRADE_COUNTRY_PK PRIMARY KEY (TRADE_COUNTRY_ID)  USING INDEX TABLESPACE  GEC);
alter table GEC_TRADE_COUNTRY
  add ( constraint GEC_TRADE_COUNTRY_UK unique (TRADE_COUNTRY_CD)  USING INDEX TABLESPACE  GEC);

comment on table GEC_TRADE_COUNTRY
  is 'Trading Country ';
comment on column GEC_TRADE_COUNTRY.TRADE_COUNTRY_ID
  is 'sequence id. And do not use this as foreign key in GEC.';	
comment on column GEC_TRADE_COUNTRY.TRADE_COUNTRY_CD
  is 'Should use this column as foreign key in other tables.';	
comment on column GEC_TRADE_COUNTRY.PREPAY_DATE_VALUE
  is 'Value: 0, 1, 2, 3';	
comment on column GEC_TRADE_COUNTRY.CUTOFF_TIME
  is 'After the time,intraday locate of this trade country will be expired. The value is like 5:30 AM';	
comment on column GEC_TRADE_COUNTRY.LOCALE
  is 'eg: America/New_York.';		
comment on column GEC_TRADE_COUNTRY.PREBORROW_ELIGIBLE_FLAG
  is 'Values: Y-es, N-o. Default value is N-o.';	  
comment on column GEC_TRADE_COUNTRY.STATUS 
is 'A-ctive, I-nactive, D-eleted. The record will not be physically deleted, but set to D-eleted status.';
comment on column GEC_TRADE_COUNTRY.VISIBLE_FLAG 
is 'Values: Y, N. A flag to indicate whether the trade country should be viewed in page or not when check to show (Only Show Active Markets).';  
  
