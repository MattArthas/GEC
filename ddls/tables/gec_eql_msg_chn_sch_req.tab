create table GEC_EQL_MSG_CHN_SCH_REQ
(
	EQUILEND_MESSAGE_ID	 NUMBER(38),
	CHAIN_SCHEDULE_REQUEST_ID	 NUMBER(38),
	LEGAL_ENTITY_ID        NUMBER(10),
