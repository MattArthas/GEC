	LAST_UPDATED_BY 	 VARCHAR2(64)
)tablespace GEC;

COMMENT ON TABLE GEC_EQL_MSG_CHN_SCH_REQ
  IS 'It store the relatiosn ship between GEC_EQUILEND_MESSAGE and GEC_CHAIN_SCHEDULE_REQUEST table.';

ALTER TABLE GEC_EQL_MSG_CHN_SCH_REQ ADD CONSTRAINTS GEC_EQL_MSG_CHN_SCH_REQ_PK
  PRIMARY KEY(EQUILEND_MESSAGE_ID) USING INDEX TABLESPACE GEC;
ALTER TABLE GEC_EQL_MSG_CHN_SCH_REQ ADD CONSTRAINTS GEC_EQL_MSG_CHN_SCH_REQ_FK1
  FOREIGN KEY(EQUILEND_MESSAGE_ID) references GEC_EQUILEND_MESSAGE(EQUILEND_MESSAGE_ID);
ALTER TABLE GEC_EQL_MSG_CHN_SCH_REQ ADD CONSTRAINTS GEC_EQL_MSG_CHN_SCH_REQ_FK2
  FOREIGN KEY(CHAIN_SCHEDULE_REQUEST_ID) references GEC_CHAIN_SCHEDULE_REQUEST(CHAIN_SCHEDULE_REQUEST_ID);
  
