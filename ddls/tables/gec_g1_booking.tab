create table GEC_G1_BOOKING
(
  G1_BOOKING_ID	NUMBER(10),
  FUND_CD         VARCHAR2(4),
  TRANSACTION_CD  VARCHAR2(3) not null,
  POS_TYPE        VARCHAR2(3) not null,
  COUNTERPARTY_CD VARCHAR2(6),
  RATE            NUMBER(14,6) default 0,
  RATE_TYPE       VARCHAR2(1),
  INDEX_CD        VARCHAR2(4),
  COLL_TYPE       VARCHAR2(1),
  COLLATERAL_CURRENCY_CD	VARCHAR2(3), 
  RECLAIM_RATE	NUMBER(9,8),
  OVERSEAS_TAX_PERCENTAGE	NUMBER(9,8),
  DOMESTIC_TAX_PERCENTAGE	NUMBER(9,8),
  LAST_UPDATED_BY VARCHAR2(64),
  LAST_UPDATED_AT DATE
)TABLESPACE GEC
;
comment on table GEC_G1_BOOKING
  is 'default values for G1 export';
comment on column GEC_G1_BOOKING.COLL_TYPE
  is 'Collateral Type ,e.g. C-ASH.';
comment on column GEC_G1_BOOKING.TRANSACTION_CD
  is 'e.g. G1B,G1L.';
comment on column GEC_G1_BOOKING.POS_TYPE
  is 'e.g. NSB,SB.';
comment on column GEC_G1_BOOKING.RATE_TYPE
  is 'R-ebate,F-ee.';
comment on column GEC_G1_BOOKING.INDEX_CD
  is 'N/A,FEDO,DFLT.';
comment on column GEC_G1_BOOKING.RECLAIM_RATE
  is '';    

alter table GEC_G1_BOOKING
  add ( constraint GEC_G1_BOOKING_PK primary key (G1_BOOKING_ID)  USING INDEX TABLESPACE  GEC);
alter table GEC_G1_BOOKING
  add constraint GEC_G1_BOOKING_FK1 foreign key (FUND_CD)
  references GEC_FUND (FUND_CD);
create unique index GEC_G1_BOOKING_IDX ON GEC_G1_BOOKING(FUND_CD, POS_TYPE, TRANSACTION_CD) TABLESPACE GEC ;