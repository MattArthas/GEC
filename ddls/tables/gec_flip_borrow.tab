CREATE TABLE GEC_FLIP_BORROW
(
FLIP_BORROW_ID	NUMBER(38),
BORROW_ID	NUMBER(38),
FLIP_TRADE_ID	NUMBER(38),
ALLOCATION_QTY	NUMBER(38),
STATUS	VARCHAR2(1),
G1_BORROW_RETURN_ID			NUMBER(38),
G1_LOAN_RETURN_ID			NUMBER(38),
LOAN_ID         		NUMBER(38),
SSGM_LOAN_ID			NUMBER(38),
SSGM_LOAN_RETURN_ID 	NUMBER(38),
LAST_UPDATED_AT	DATE,
LAST_UPDATED_BY	VARCHAR2(64)
) TABLESPACE GEC
;
  
COMMENT ON TABLE GEC_FLIP_BORROW
  IS 'This table is used import flip borrow response.';
  
COMMENT ON COLUMN GEC_FLIP_BORROW.STATUS
  IS 'Values: N-ew (initial status), P-ending(Means allocate completed, but display Book on Flip Borrow UI based on requirement), C-ancel.';
  
ALTER TABLE GEC_FLIP_BORROW
  ADD CONSTRAINT GEC_FLIP_BORROW_PK PRIMARY KEY (FLIP_BORROW_ID)  USING INDEX TABLESPACE  GEC;

ALTER TABLE GEC_FLIP_BORROW
  ADD CONSTRAINT FLIP_BORROW_FK1 FOREIGN KEY (FLIP_TRADE_ID)
  REFERENCES GEC_FLIP_TRADE (FLIP_TRADE_ID);
  
ALTER TABLE GEC_FLIP_BORROW
  ADD CONSTRAINT FLIP_BORROW_FK2 FOREIGN KEY (BORROW_ID)
  REFERENCES GEC_BORROW (BORROW_ID);
  
create index GEC_FLIP_BORROW_IDX1 on GEC_FLIP_BORROW (FLIP_TRADE_ID, BORROW_ID) TABLESPACE GEC;