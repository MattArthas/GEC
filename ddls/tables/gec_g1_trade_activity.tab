ALTER TABLE GEC_G1_TRADE_ACTIVITY ADD CONSTRAINTS GEC_G1_TRADE_ACTIVITY_PK
  PRIMARY KEY(G1_TRADE_ACTIVITY_ID) USING INDEX TABLESPACE GEC;

ALTER TABLE GEC_G1_TRADE_ACTIVITY ADD CONSTRAINT GEC_G1_TRADE_ACTIVITY_FK1 FOREIGN KEY (G1_MESSAGE_ID)
  REFERENCES GEC_G1_MESSAGE (G1_MESSAGE_ID);