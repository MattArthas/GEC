  SFP_LEAGAL_ENTITY_ID	VARCHAR2(40),
  BGNREF	VARCHAR2(80),
  CPTY	VARCHAR2(6),
  STOCK	VARCHAR2(12),
  COLL_FLG	VARCHAR2(1),
  CASH	VARCHAR2(1),
  LNCUR	VARCHAR2(3),
  ACT_TYPE	VARCHAR2(3),
  EFF_DT	NUMBER(8),
  TRADE	NUMBER(8),
  CSDT	NUMBER(8),
  SSDT	NUMBER(8),
  LNRATE	NUMBER(14,6),
  ACT_QTY	NUMBER(38),
  DIV_AGE_6DP	NUMBER(10,6),
  DIV_DOM_6DP	NUMBER(10,6),
  BL	VARCHAR2(1),
  CNTRY_ISS	VARCHAR2(3),
