create global temporary table GEC_SUM_OF_ORDERS_TEMP
(
  BUSINESS_DATE         NUMBER(8),
  INVESTMENT_MANAGER_CD VARCHAR2(32),
  FUND_CD               VARCHAR2(4),
  TRANSACTION_CD        VARCHAR2(10),
  STATUS                VARCHAR2(1),
  CLIENT_CD             VARCHAR2(32),
  SUMOFSHARE_QTY        NUMBER(38) default 0,
  ASSET_ID              NUMBER(38)
)
on commit delete rows;
comment on table GEC_SUM_OF_ORDERS_TEMP
  is 'local temp sum table';
create index GEC_SUM_OF_ORDERS_TEMP_IDX1 on GEC_SUM_OF_ORDERS_TEMP (BUSINESS_DATE, INVESTMENT_MANAGER_CD, CLIENT_CD, ASSET_ID, FUND_CD);

