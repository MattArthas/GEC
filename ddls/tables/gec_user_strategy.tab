create table GEC_USER_STRATEGY
(
  USER_ID         VARCHAR2(64) not null,
  STRATEGY_ID     NUMBER(38) not null,
  LAST_UPDATED_BY VARCHAR2(64),
  LAST_UPDATED_AT DATE
)TABLESPACE GEC
;
alter table GEC_USER_STRATEGY
  add ( constraint GEC_USER_STRATEGY_PK primary key (USER_ID, STRATEGY_ID)  USING INDEX  TABLESPACE GEC);
alter table GEC_USER_STRATEGY
  add constraint GEC_USER_STRATEGY_FK1 foreign key (USER_ID)
  references GEC_USER (USER_ID);
alter table GEC_USER_STRATEGY
  add constraint GEC_USER_STRATEGY_FK2 foreign key (STRATEGY_ID)
  references GEC_STRATEGY (STRATEGY_ID);

comment on table GEC_USER_STRATEGY
  is 'User Strategy relation table.';
