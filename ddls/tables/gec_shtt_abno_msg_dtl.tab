CREATE TABLE GEC_SHTT_ABNO_MSG_DTL
(
	EQUILEND_MESSAGE_ID	  NUMBER(38),
	FROM_CORP_ID          NUMBER(10),
	FROM_SUBACCOUNT_ID    VARCHAR2(10),
	FROM_LEGAL_ENTITY_ID  NUMBER(10),
	FROM_TRADER_ID        VARCHAR2(10),
	TO_CORP_ID            NUMBER(10),
	TO_SUBACCOUNT_ID	  VARCHAR2(10),
	TO_LEGAL_ENTITY_ID	  NUMBER(10),
	TO_TRADER_ID          VARCHAR2(10),
	BROKER_CD	          VARCHAR2(6),
	EQUILEND_ID	          NUMBER(15),
	SECURITY_ID	          VARCHAR2(12), --LEND 
	SECURITY_ID_TYPE	  VARCHAR2(1),  --LENDER
	BORROW_QTY 	          NUMBER(38),
	RATE	              NUMBER(14,6), -- price rate amount
	RATE_TYPE             VARCHAR2(2),-- BO,FI,BE
	BANCHMARK             VARCHAR2(8),
	RESET_INTERVAL        NUMBER(3),
	FEE                   NUMBER(14,6),-- PRICE FEE AMOUNT
	FEE_TYPE              VARCHAR2(2),-- VALUE IS FR - Fixed Rate, PS - Price Per Share, PR-Par 
	PRICE_CURRENCY_CD     VARCHAR2(3),
	CASHAMOUNT            NUMBER(17,2),
	RECLAIM_RATE	      NUMBER(9,8), -- RECLAIM RATE		
	MOD2    			  VARCHAR2(1), --Y/N
	TAX_VOUCHER_DETAILS   NUMBER(9,5),
	CUM_OR_EX_DIVIDEND    VARCHAR2(1),
	MINIMUM_FEE	          NUMBER(14,2),
	PREPAY_RATE           NUMBER(14,6),
	BILLING_PRICE	      NUMBER(13,6),
	MINIMUM_FEE_CD	      VARCHAR2(3), --BILLING currency
	BILLING_FXRATE        NUMBER(16,7),		
	BILLING_CASHAMOUNT	  NUMBER(17,2), 
	BILL_DERIVATION_IND   VARCHAR2(1), -- VALUE IS M-arket/C-ollateral
	EQL_COLLATERAL_TYPE	  VARCHAR2(2),
	EQL_COLLATERAL_TYPE_DESC VARCHAR2(3),
	COLLATERAL_MARGIN     NUMBER(5,4),--DIVE 100
	AMOUNT	              NUMBER(28,6),--LOAN_AMOUT
	CONTRACT_PRICE        NUMBER(13,6),
	CONTRACT_VALUE        NUMBER(17,2),
	COLLATERAL_CURRENCY_CD	VARCHAR2(3),
	COLLATERAL_FXRATE     NUMBER(16,7),
	TRADE_DATE	          NUMBER(8),
	SETTLE_DATE	          NUMBER(8),
	PREPAY_DATE	          NUMBER(8),
	TERM_DATE             NUMBER(8),
	TERM_TYPE             VARCHAR2(1),--F-ixed/I-nactive
	CALLABLE_DATE         NUMBER(8),
	SETTLEMENT_TYPE       VARCHAR2(3), --BNY,CHA, DVB,DVP, EUR, FOP
	BORR_SETTLE_INSTRID   VARCHAR2(10),
	LEND_SETTLE_INSTRID   VARCHAR2(10),
    LOG_NUMBER	          VARCHAR2(20), --internalRefId 
	PRIVATE_COMMENTS      VARCHAR2(128),
	CALLABLE_INDICATOR    VARCHAR2(1),--Y/N
	CALLBACK_INDICATOR    VARCHAR2(1),--Y/N
	STATUS                VARCHAR2(1) default 'N' not null,
	STATUS_MSG            VARCHAR2(64),
	HOLDBACK_FLAG         VARCHAR2(1) default 'N' not null,
    LAST_UPDATED_AT      DATE,
    LAST_UPDATED_BY      VARCHAR2(64)
) TABLESPACE GEC;

COMMENT ON TABLE GEC_SHTT_ABNO_MSG_DTL
   IS 'This table is to store the equilend"s accept message detail, it has been paresed to object and store in this table.';
COMMENT ON COLUMN GEC_SHTT_ABNO_MSG_DTL.STATUS
   IS 'Value is N-ew/E-rror, for DEV old data, set value is N';
COMMENT ON COLUMN GEC_SHTT_ABNO_MSG_DTL.HOLDBACK_FLAG
   IS 'Value is Y-es/N-o,for DEV old data, set value is N';   
COMMENT ON COLUMN GEC_SHTT_ABNO_MSG_DTL.EQUILEND_MESSAGE_ID
   IS 'The same value with GEC_EQUILEND_MESSAGE"EQUILEND_MESSAGE_ID, it indicate that this record is the detail of GEC_EQUILEND_MESSAGE" message';
COMMENT ON COLUMN    GEC_SHTT_ABNO_MSG_DTL.SECURITY_ID_TYPE
   IS 'Value is S, C, T, Q, I. Unique identifier for a security id type. The supported types in the platform are SEDOL, CUSIP, TICKER, QUICK and ISIN.';
ALTER TABLE GEC_SHTT_ABNO_MSG_DTL ADD CONSTRAINTS GEC_SHTT_ABNO_MSG_DTL_PK 
   PRIMARY KEY(EQUILEND_MESSAGE_ID) USING INDEX  TABLESPACE GEC;
ALTER TABLE GEC_SHTT_ABNO_MSG_DTL ADD CONSTRAINTS GEC_SHTT_ABNO_MSG_DTL_FK
   FOREIGN KEY(EQUILEND_MESSAGE_ID) references GEC_EQUILEND_MESSAGE(EQUILEND_MESSAGE_ID);
COMMENT ON COLUMN GEC_SHTT_ABNO_MSG_DTL.SECURITY_ID 
   IS 'lender"s security id';
COMMENT ON COLUMN GEC_SHTT_ABNO_MSG_DTL.SECURITY_ID_TYPE 
   IS 'lender"s security id type';
COMMENT ON COLUMN GEC_SHTT_ABNO_MSG_DTL.RATE
   IS 'price"s rate"s amount';
COMMENT ON COLUMN GEC_SHTT_ABNO_MSG_DTL.RATE_TYPE 
   IS 'value is "BO,FI,BE",  price"s rate"s rate type'; 
COMMENT ON COLUMN GEC_SHTT_ABNO_MSG_DTL.FEE
   IS 'PRICE" FEE" AMOUNT';
COMMENT ON COLUMN GEC_SHTT_ABNO_MSG_DTL.FEE_TYPE
   IS 'VALUE IS FR - Fixed Rate, PS - Price Per Share, PR-Par. price fee feetype';
COMMENT ON COLUMN GEC_SHTT_ABNO_MSG_DTL.PRICE_CURRENCY_CD 
   IS 'price"cashpayment"currency';
COMMENT ON COLUMN GEC_SHTT_ABNO_MSG_DTL.CASHAMOUNT 
   IS 'price"cashpayment"cashAmount';
COMMENT ON COLUMN GEC_SHTT_ABNO_MSG_DTL.RECLAIM_RATE 
   IS 'dividendRate, the value is equilend dividendRate is divided by 100';   
COMMENT ON COLUMN GEC_SHTT_ABNO_MSG_DTL.BILLING_PRICE 
   IS 'billing"billingprice';
COMMENT ON COLUMN GEC_SHTT_ABNO_MSG_DTL.MINIMUM_FEE_CD
   IS  'billing"currency';
COMMENT ON COLUMN GEC_SHTT_ABNO_MSG_DTL.BILL_DERIVATION_IND
   IS  'VALUE IS M-arket/C-ollateral';
COMMENT ON COLUMN GEC_SHTT_ABNO_MSG_DTL.EQL_COLLATERAL_TYPE
   IS 'collateral"collType';
COMMENT ON COLUMN GEC_SHTT_ABNO_MSG_DTL.COLLATERAL_MARGIN 
   IS 'Value is the equilend margin value is divided by 100';
COMMENT ON COLUMN GEC_SHTT_ABNO_MSG_DTL.AMOUNT 
   IS 'Value is collateral" cashAmout/loanAmout';
COMMENT ON COLUMN GEC_SHTT_ABNO_MSG_DTL.COLLATERAL_CURRENCY_CD 
   IS 'collateral currency';
COMMENT ON COLUMN GEC_SHTT_ABNO_MSG_DTL.BILLING_CASHAMOUNT    
   IS 'billing"cashamount';   
