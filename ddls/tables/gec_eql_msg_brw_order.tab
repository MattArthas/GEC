create table GEC_EQL_MSG_BRW_ORDER
(
	EQUILEND_MESSAGE_ID	 NUMBER(38),
	BORROW_ORDER_ID	     NUMBER(38),
	EQUILEND_ID            NUMBER(15),
	LAST_UPDATED_AT	     DATE,
	LAST_UPDATED_BY 	 VARCHAR2(64)
)tablespace GEC;

COMMENT ON TABLE GEC_EQL_MSG_BRW_ORDER
  IS 'It store the relatiosn ship between GEC_EQUILEND_MESSAGE and GEC_BORROW_ORDER table.';

ALTER TABLE GEC_EQL_MSG_BRW_ORDER ADD CONSTRAINTS GEC_EQL_MSG_BRW_ORDER_PK
  PRIMARY KEY(EQUILEND_MESSAGE_ID) USING INDEX TABLESPACE GEC;
ALTER TABLE GEC_EQL_MSG_BRW_ORDER ADD CONSTRAINTS GEC_EQL_MSG_BRW_ORDER_FK1
  FOREIGN KEY(EQUILEND_MESSAGE_ID) references GEC_EQUILEND_MESSAGE(EQUILEND_MESSAGE_ID);
ALTER TABLE GEC_EQL_MSG_BRW_ORDER ADD CONSTRAINTS GEC_EQL_MSG_BRW_ORDER_FK2
  FOREIGN KEY(BORROW_ORDER_ID) references GEC_BORROW_ORDER(BORROW_ORDER_ID);
comment on column   GEC_EQL_MSG_BRW_ORDER.EQUILEND_ID
  is 'It is the value of the EQUILEND"S ID.';  
