create table GEC_USER_DATAGRID_PREF
(
  USER_ID         VARCHAR2(64) not null,
  DATAGRID_ID     VARCHAR2(50) not null,
  COLUMN_ID       VARCHAR2(50) not null,
  DISPLAY_WIDTH   NUMBER(6,3),
  VISIBLE_FLAG    CHAR(1),
  DISPLAY_ORDER   NUMBER(3),
  LAST_UPDATED_BY VARCHAR2(64),
  LAST_UPDATED_AT DATE
)TABLESPACE GEC
;
alter table GEC_USER_DATAGRID_PREF
  add ( constraint GEC_USER_DATAGRID_PREF_PK primary key (USER_ID, DATAGRID_ID, COLUMN_ID)  USING INDEX TABLESPACE GEC) ;
alter table GEC_USER_DATAGRID_PREF
  add constraint GEC_USER_DATAGRID_PREF_FK1 foreign key (DATAGRID_ID, COLUMN_ID)
  references GEC_DATAGRID_COLUMN (DATAGRID_ID, COLUMN_ID);
  
  comment on table GEC_USER_DATAGRID_PREF
  is 'User preference for datagrid setting.';

