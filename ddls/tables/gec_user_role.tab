  ROLE_CD         VARCHAR2(30) not null,
  LAST_UPDATED_BY VARCHAR2(64),
  LAST_UPDATED_AT DATE
)TABLESPACE GEC
;
alter table GEC_USER_ROLE
  add ( constraint GEC_USER_ROLE_PK primary key (USER_ID, ROLE_CD)  USING INDEX TABLESPACE  GEC);
alter table GEC_USER_ROLE
