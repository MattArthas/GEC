CREATE TABLE GEC_STRATEGY_PROFILE
(
	STRATEGY_PROFILE_ID  		NUMBER(38) 		NOT NULL,
	STRATEGY_ID 				NUMBER(38) 		NOT NULL,
	TRADE_COUNTRY_CD 			VARCHAR2(50) 	NOT NULL,
	SB_PERCENTAGE 				NUMBER(7,6) 	DEFAULT 1,
	SB_RAL_PERCENTAGE 			NUMBER(7,6) 	DEFAULT 1 ,
	SFA_PERCENTAGE 				NUMBER(7,6) 	DEFAULT 1 ,
	SURROGATE_PERCENTAGE 		NUMBER(7,6) 	DEFAULT 1 ,
	EXT2_PERCENTAGE 			NUMBER(7,6) 	DEFAULT 1 ,
	LIQUID_PERCENTAGE 			NUMBER(7,6) 	DEFAULT 1 ,
	MIN_SP_RATE 				NUMBER(9,6),
	GC_RATE                     NUMBER(9,6),
	STATUS 						VARCHAR2(1) 	NOT NULL,
	HOLDBACK_FLAG				VARCHAR2(1)		NOT NULL,
	SCHEDULED_AT				VARCHAR2(8),	
	AVAILABILITY_REGIONS_STR	VARCHAR2(20),	
	ASSET_TYPES_STR				VARCHAR2(32),
	LOCATE_SWITCH 		  		VARCHAR2(1) default 'N' not null,
	LAST_UPDATED_BY 			VARCHAR2(64),
	LAST_UPDATED_AT 			DATE
)  TABLESPACE GEC
;
ALTER TABLE GEC_STRATEGY_PROFILE
  ADD( CONSTRAINT GEC_STRATEGY_PROFILE_PK PRIMARY KEY (STRATEGY_PROFILE_ID)  USING INDEX TABLESPACE  GEC);
ALTER TABLE GEC_STRATEGY_PROFILE
  ADD ( constraint GEC_STRATEGY_PROFILE_UK unique (STRATEGY_ID,TRADE_COUNTRY_CD)  USING INDEX TABLESPACE  GEC);	
ALTER TABLE GEC_STRATEGY_PROFILE 
  ADD CONSTRAINT GEC_STRATEGY_PROFILE_FK foreign key (STRATEGY_ID) REFERENCES GEC_STRATEGY (STRATEGY_ID);  
ALTER TABLE GEC_STRATEGY_PROFILE 
  ADD CONSTRAINT GEC_STRATEGY_PROFILE_FK1 foreign key (TRADE_COUNTRY_CD) REFERENCES GEC_TRADE_COUNTRY (TRADE_COUNTRY_CD);

comment on table GEC_STRATEGY_PROFILE
  is 'Strategy Profile ';
comment on column GEC_STRATEGY_PROFILE.STRATEGY_PROFILE_ID
  is 'sequence id. And do not use this as foreign key in GEC';
comment on column GEC_STRATEGY_PROFILE.SB_PERCENTAGE
  is 'default value 1';
comment on column GEC_STRATEGY_PROFILE.SB_RAL_PERCENTAGE
  is 'default value 1';
comment on column GEC_STRATEGY_PROFILE.SFA_PERCENTAGE
  is 'default value 1';
comment on column GEC_STRATEGY_PROFILE.SURROGATE_PERCENTAGE
  is 'default value 1';
comment on column GEC_STRATEGY_PROFILE.EXT2_PERCENTAGE
  is 'default value 1';     
comment on column GEC_STRATEGY_PROFILE.LIQUID_PERCENTAGE
  is 'control the availability quantity for GC security when liquidity flag is N in security master.';
comment on column GEC_STRATEGY_PROFILE.STATUS
  is 'Values: A-ctive, I-nactive';      
--comment on column GEC_STRATEGY_PROFILE.COLLATERAL_TYPE
--  is 'Values: CASH, etc.It is corresponding to IR field: coll_type';    
comment on column GEC_STRATEGY_PROFILE.HOLDBACK_FLAG
  is 'Values: Y-es, N-o. Y-es means the locates submitted after cutoff time will not be processed until next scheduled_at time.'; 
comment on column GEC_STRATEGY_PROFILE.SCHEDULED_AT
  is 'The start time to process the pending future locates. The value is like 5:30 AM';
comment on column GEC_STRATEGY_PROFILE.AVAILABILITY_REGIONS_STR 
is 'BOS, LON, PAC. It means a certain time of loading availability will take effect for this strategy profile. Eg: BOS is corresponding to UI 8:00 am, so the loading availability at 8:00 am will take effect for this strategy profile.';
comment on column GEC_STRATEGY_PROFILE.LOCATE_SWITCH 
is 'If the value is Y, when locate imported via IM Portal, API, Eze Castle, Secure Transport, a notification email will send to trader.';   
      
      


  
  
  
  
  
  




  
  





  
